package com.faiz.test;

import java.util.ArrayList;
import java.util.Collections;
import java.util.List;

import com.faiz.model.Employee;

public class SortingTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Employee e1 = new Employee(1, "aTestName", "dLastName", 34);
        Employee e2 = new Employee(2, "nTestName", "pLastName", 30);
        Employee e3 = new Employee(3, "kTestName", "sLastName", 31);
        Employee e4 = new Employee(4, "yTestName", "zLastName", 25);
        Employee e5 = new Employee(6, "sTestName", "vLastName", 25);
        Employee e6 = new Employee(5, "nTestName", "kLastName", 25);
 
        List<Employee> employees = new ArrayList<Employee>();
        employees.add(e2);
        employees.add(e3);
        employees.add(e1);
        employees.add(e4);
        employees.add(e5);
        employees.add(e6);
 
        // UnSorted List
        System.out.println("effore Sorting");
        System.out.println(employees);
 
        System.out.println("After effore Sorting");
        Collections.sort(employees);
 
        // Default Sorting by employee id
        System.out.println(employees);

	}

}
